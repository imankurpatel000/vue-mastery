{
  "name": "vue-mastery",
  "version": "1.0.0",
  "description": "Vue Mastery website using Nuxt with Firebase to auth users without a server",
  "author": "Pierre Schweiger <schweiger.pierre@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "analyse": "nuxt build -a",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "check": "node functions/utils/checkSubscriptions.js",
    "deploy": "npm run generate && firebase deploy --only hosting --debug"
