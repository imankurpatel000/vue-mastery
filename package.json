    "vue-vimeo-player": "git+https://github.com/pieer/vue-vimeo-player.git",
    "vuexfire": "^3.1.2"
  },
  "devDependencies": {
    "@firebase/app-types": "0.4.3",
    "@nuxtjs/google-tag-manager": "^2.2.0",
    "@nuxtjs/markdownit": "^1.2.5",
    "@nuxtjs/pwa": "^3.0.0-beta.16",
    "@nuxtjs/sitemap": "^1.2.0",
    "@nuxtjs/style-resources": "^1.0.0",
